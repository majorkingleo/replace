  VSize=AP_SHRINK
]

MSKGROUP GrpHorizBottomRight [
  Align=GEOM_H_ALIGN
  HJoin=GEOM_H_RJOINED
  VJoin=GEOM_V_BJOINED
  HSize=AP_EXPAND
  VSize=AP_EXPAND
]

