  VJoin=GEOM_V_BJOINED
]

# ****************************************************************************
# *         M A S K   										   	             *
# ****************************************************************************
# START Mask ME_ANMELD

MSKDIALOG ME_ANMELD [
  Unique 		= 1
  ShellRealize 	= 1
  KeepPos 		= 0
  KeepSize 		= 0
  SuOnly 		= 0
  NoTopPlate 	= 1
  Title			= "Anmeldedialog"
  
  MSKATOM [
    Version = "%W% %G% %U%"
  ]
  
  MSKDESCR Filler []
  
  MSKDESCR GrpHoriz [
    
    MSKDESCR GrpVert [
      
### Filterteil ###
      MSKDESCR GrpFrame_Anmeld_Filter [
        MSKDESCR Filler []
        MSKDESCR GrpHoriz [
          MSKDESCR ZOVW_PersNr_t [
            Key = 0
            CMatch = "-ANMELD_INP-"
            Label   = "PersNr"
            MSKATOM [
              OpmEfDbName   = "ZOVW.PersNr"
              OpmPorderBy1  = "ZOVW.PersNr"
              OpmTable      = "ZOVW"
              OwCallback1   = CbSqlOm
            ]
          ]
          MSKDESCR GrpHorizBottomRight [
            MSKDESCR Read_F [
              CMatch = "-ANMELD_INP-"
              IsDefault= True
            ]
          ] 
        ]
      ]
      
### Datenteil ###
      MSKDESCR GrpFrame_Anmeld_Data [
        MSKDESCR Filler []
        MSKDESCR GrpHoriz [
          MSKDESCR ME_ANMELD_SpreadSheet [
            CMatch = "-ANMELD_INP-"
          ]
          MSKDESCR Filler []
          MSKDESCR GrpHorizBottomRight [
            MSKDESCR Info_F [
              CMatch = "-ANMELD_INP-"
            ]
          ] 
        ] 
      ]
      
### Aktionsteil ###
      MSKDESCR GrpFrame_Anmeld_Action [
        MSKDESCR Filler []
        MSKDESCR GrpVert [
          MSKDESCR GrpHoriz [
            MSKDESCR Marker-1 []
            MSKDESCR GrpVert [
              MSKDESCR ZOVW_PersNr_t [
                Key = 1
                CMatch = "-ANMELD_INP-ANMELD_ZOVW-"
                Label   = "PersNr"
              ]
              MSKDESCR ZOVW_ABer_SOLL_VKp_t [
                CMatch = "-ANMELD_INP-ANMELD_ZOVW-"
                Label   = "Von Kp"
                MSKATOM [
                  OwCallback1 = CbGeneral_KpIdx
                  OwCallback2 = CbGeneral_KpIdx
                ]
              ]
              MSKDESCR ZOVW_SId_Name_t [
                CMatch = "-ANMELD_INP-ANMELD_ZOVW-ANMELD_LS-"
                Label   = "Scanner-Id"
                 MSKATOM [
#                  OpmEfDbName   = "SCR.Id_Name"
#                  OpmPorderBy1  = "SCR.Id_Name"
#                  OpmTable      = "SCR"
#                  OwCallback1   = CbSqlOm
                   OwCallback1   = CbGeneral_SId_Name
                 ]
              ]
            ]
            MSKDESCR Marker-2 []
            MSKDESCR GrpVert [
              MSKDESCR DummyLabel_ANMELD []
              MSKDESCR ZOVW_ABer_SOLL_NKp_t [
                CMatch = "-ANMELD_INP-ANMELD_ZOVW-"
                Label   = "Bis Kp"
                MSKATOM [
                  OwCallback1 = CbGeneral_KpIdx
                ]
              ]
              MSKDESCR ZOVW_Kpd_Bz_SOLL_t [
                CMatch = "-ANMELD_INP-ANMELD_ZOVW-ANMELD_LS-"
                Label   = "Betriebszustand"
              ]
            ]
          ]
          MSKDESCR ZOVW_InfoTxt1_t [
            CMatch = "-ANMELD_INP-ANMELD_ZOVW-"
          ]
          MSKDESCR SepHoriz []
          MSKDESCR GrpHoriz [
            MSKDESCR GrpRow [
              MSKDESCR ANMELD_Scanner_F [
                CMatch = "-ANMELD_INP-"
              ]
              MSKDESCR ANMELD_SC2VPLI_F [
                CMatch = "-ANMELD_INP-"
              ]
            ]
            MSKDESCR GrpHorizBottomRight [
              MSKDESCR GrpRow1Column2 [
                MSKDESCR Delete_F [
                  CMatch = "-ANMELD_INP-"
                ]
                MSKDESCR ANMELD_Adopt_F [
                  CMatch = "-ANMELD_INP-"
                ]
              ]
            ]
          ]
          
        ]
      ]
      
    ]
    
### Buttons rechts ###
    MSKDESCR GrpVert [
      MSKDESCR GrpColumn [
        MSKDESCR Ok_F [
          CMatch = "-ANMELD_INP-"
        ]
        MSKDESCR ClrFld_F [
          CMatch = "-ANMELD_INP-"
        ]
        MSKDESCR Cancel_F [
          Callback = OwClose
          IsCancel = True
        ]
        
        MSKDESCR Filler []
        MSKDESCR Filler []
        MSKDESCR Filler []
        MSKDESCR Filler []
        MSKDESCR Filler []
        MSKDESCR Filler []
        MSKDESCR Filler []
        MSKDESCR Filler []
        MSKDESCR Filler []
        
        MSKDESCR ANMELD_PBLEC_F [
          CMatch = "-ANMELD_INP-"
        ]
      ]
    ]
    
  ]
]
# END Mask ME_ANMELD
